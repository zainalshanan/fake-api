{
  "name": "fake-api",
  "version": "2.0.0",
  "description": "A powerful tool that automatically generates mock API endpoints, fake data, and starts a server based on your Swagger/OpenAPI specification.",
  "main": "dist/src/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "generate": "node dist/src/index.js generate",
    "mock": "node dist/src/index.js mock",
    "serve": "tsc &&node dist/src/index.js serve",
    "start": "npm run build && npm run serve",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.1",
    "axios": "^1.6.7",
    "chalk": "^5.4.1",
    "commander": "^11.1.0",
    "express": "^4.18.2",
    "lowdb": "^7.0.1",
    "openapi-data-validator": "^2.0.52",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/express": "^4.17.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.1.6",
    "@types/pluralize": "^0.0.33",
    "@types/rimraf": "^3.0.2",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-istanbul": "^3.1.4",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/ui": "^3.1.4",
    "openapi-types": "^12.1.3",
    "portfinder": "^1.0.37",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.1",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  }
}
