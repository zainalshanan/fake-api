{
    "name": "fake-api",
    "version": "1.0.0",
    "description": "A powerful tool that automatically generates mock API endpoints, fake data, and starts a server based on your Swagger/OpenAPI specification.",
    "main": "dist/src/index.js",
    "type": "module",
    "scripts": {
        "build": "tsc",
        "generate": "node dist/src/index.js generate",
        "mock": "node dist/src/index.js mock",
        "serve": "node dist/src/index.js serve",
        "dev:generate": "ts-node src/index.ts generate",
        "dev:mock": "ts-node src/index.ts mock",
        "dev:serve": "ts-node src/index.ts serve",
        "start": "npm run build && npm run serve",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:ui": "vitest --ui",
        "coverage": "vitest run --coverage"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@apidevtools/swagger-parser": "^10.1.1",
        "@types/express": "^4.17.14",
        "axios": "^1.6.7",
        "chalk": "^5.4.1",
        "commander": "^11.1.0",
        "express": "^4.18.2",
        "express-openapi-validator": "^5.5.0",
        "lowdb": "^7.0.1",
        "openapi-types": "^12.1.3",
        "yaml": "^2.3.4"
    },
    "devDependencies": {
        "@faker-js/faker": "^8.4.1",
        "@types/node": "^20.1.6",
        "@types/uuid": "^10.0.0",
        "@vitest/coverage-istanbul": "^3.1.4",
        "@vitest/coverage-v8": "^3.1.4",
        "@vitest/ui": "^3.1.4",
        "ts-node": "^10.9.2",
        "tsx": "^4.19.4",
        "typescript": "^5.3.3",
        "uuid": "^11.1.0",
        "vitest": "^3.1.4"
    }
}
